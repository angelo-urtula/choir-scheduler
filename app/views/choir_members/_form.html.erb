<%= form_for @choir_member do |f|%>
    <%= render partial: 'choir_leaders/form', locals: {f: f} %>
    <%= render partial: 'add_info', locals: {f: f} %>
    <% if !@choir_member[:uid] %>
    <%= render partial: 'choir_leaders/login_info', locals: {f: f} %>
    <% end %>
    <%= f.submit %>
    <% end %>